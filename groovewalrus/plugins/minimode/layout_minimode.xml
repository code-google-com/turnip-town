<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<resource xmlns="http://www.wxwindows.org/wxxrc" version="2.3.0.1">
	<object class="wxPanel" name="m_pa_plugin_minimode">
		<style>wxTAB_TRAVERSAL</style>
		<size>614,56</size>
		<bg>#000000</bg>
		<object class="wxBoxSizer">
			<orient>wxHORIZONTAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
				<border>0</border>
				<object class="wxStaticBitmap" name="m_bm_minimode_grabber">
					<tooltip>Drag (Right-click to close)</tooltip>
					<bitmap>m_grabber.png</bitmap>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALIGN_CENTER_VERTICAL|wxTOP|wxBOTTOM|wxLEFT</flag>
				<border>5</border>
				<object class="wxStaticBitmap" name="m_bm_minimode_backward">
					<bitmap>m_backward.png</bitmap>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALIGN_CENTER_VERTICAL|wxTOP|wxBOTTOM</flag>
				<border>5</border>
				<object class="wxStaticBitmap" name="m_bm_minimode_play">
					<bitmap>m_play.png</bitmap>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxALIGN_CENTER_VERTICAL|wxTOP|wxBOTTOM|wxRIGHT</flag>
				<border>5</border>
				<object class="wxStaticBitmap" name="m_bm_minimode_forward">
					<bitmap>m_forward.png</bitmap>
				</object>
			</object>
			<object class="sizeritem">
				<option>1</option>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<object class="wxBoxSizer">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<option>1</option>
						<flag>wxEXPAND|wxALL</flag>
						<border>5</border>
						<object class="wxPanel" name="m_pa_minimode_status" subclass="main_controls.playback_panel.PlaybackPanel">
							<style>wxTAB_TRAVERSAL</style>
							<bg>#f0f0f0</bg>
						</object>
					</object>
				</object>
			</object>
		</object>
	</object>
</resource>
