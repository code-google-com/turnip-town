<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<resource xmlns="http://www.wxwindows.org/wxxrc" version="2.3.0.1">
	<object class="wxPanel" name="m_pa_plugin_played">
		<style>wxTAB_TRAVERSAL</style>
		<size>487,307</size>
		<object class="wxBoxSizer">
			<orient>wxVERTICAL</orient>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<object class="wxBoxSizer">
					<orient>wxHORIZONTAL</orient>
					<object class="sizeritem">
						<option>1</option>
						<flag>wxTOP|wxRIGHT|wxLEFT|wxEXPAND</flag>
						<border>5</border>
						<object class="wxStaticText" name="m_st_played_header">
							<fg>#0080ff</fg>
							<font>
								<size>11</size>
								<family>default</family>
								<style>normal</style>
								<weight>bold</weight>
								<underlined>0</underlined>
							</font>
							<label>Played</label>
						</object>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<object class="wxStaticBitmap" name="m_bm_played_close">
							<size>22,22</size>
							<bitmap>emblem-unreadable.png</bitmap>
						</object>
					</object>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxEXPAND | wxALL</flag>
				<border>5</border>
				<object class="wxStaticLine" name="m_staticline4">
					<style>wxLI_HORIZONTAL</style>
				</object>
			</object>
			<object class="sizeritem">
				<option>1</option>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<object class="wxBoxSizer">
					<orient>wxVERTICAL</orient>
					<object class="sizeritem">
						<option>1</option>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<object class="wxListCtrl" name="m_lc_played_list">
							<style>wxLC_REPORT</style>
						</object>
					</object>
				</object>
			</object>
			<object class="sizeritem">
				<option>0</option>
				<flag>wxEXPAND</flag>
				<border>5</border>
				<object class="wxBoxSizer">
					<orient>wxHORIZONTAL</orient>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>5</border>
						<object class="wxBitmapButton" name="m_bb_played_add_all">
							<style>wxBU_AUTODRAW</style>
							<size>100,-1</size>
							<tooltip>Add all to playlist</tooltip>
							<bitmap>list-add-all.png</bitmap>
							<default>0</default>
						</object>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>5</border>
						<object class="wxBitmapButton" name="m_bb_played_add">
							<style>wxBU_AUTODRAW</style>
							<tooltip>Add selected to playlist</tooltip>
							<bitmap>list-add.png</bitmap>
							<default>0</default>
						</object>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL</flag>
						<border>5</border>
						<object class="wxSpinButton" name="m_sb_played_list">
							<style>wxSP_HORIZONTAL</style>
							<tooltip>previous/next results</tooltip>
						</object>
					</object>
					<object class="spacer">
						<option>0</option>
						<flag>wxEXPAND</flag>
						<border>5</border>
						<size>20,0</size>
					</object>
					<object class="sizeritem">
						<option>0</option>
						<flag>wxALL|wxALIGN_CENTER_VERTICAL</flag>
						<border>5</border>
						<object class="wxRadioBox" name="m_rx_played_radio">
							<style>wxRA_SPECIFY_COLS</style>
							<label></label>
							<selection>0</selection>
							<content>
								<item>Played Recently</item>
								<item>Played Most</item>
							</content>
							<dimension>2</dimension>
						</object>
					</object>
				</object>
			</object>
		</object>
	</object>
</resource>
